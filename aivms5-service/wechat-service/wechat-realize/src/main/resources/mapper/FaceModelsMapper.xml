  <!-- 根据访客人脸ID查询访客信息 -->
  <select id="findVisitorByFaceid" resultType="com.qlhx.model.VVisitor">
   SELECT v.*,a.sitePhoto,a.CardNum FROM
		u_visitor v LEFT JOIN u_access_record a
		on v.id = a.vID  WHERE  v.id in(SELECT uid from u_user_facemodels where facemodelid  in (${faceids}))
		ORDER BY a.id desc LIMIT 1
  </select>


