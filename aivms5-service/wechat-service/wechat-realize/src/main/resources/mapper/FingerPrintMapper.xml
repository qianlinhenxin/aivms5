  <!-- 根据访客手指ID查询访客信息 -->
  <select id="findVisitorByfingerids"  parameterType="com.qlhx.model.FingerPrint"  resultType="com.qlhx.model.VVisitor">
 SELECT v.*,a.sitePhoto,a.CardNum FROM
    u_visitor v LEFT JOIN u_access_record a
    on v.id = a.vID  WHERE  v.id in(SELECT uid from u_user_fingerprints where fingerprintid  in (${fingerprintid}) and utype =#{utype} )
    ORDER BY a.id desc LIMIT 1
  </select>

